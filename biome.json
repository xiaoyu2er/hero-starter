{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["ultracite"],
  "javascript": {
    "globals": [""]
  },
  "files": {
    "ignore": [".vinxi/**", "node_modules/**", "src/routeTree.gen.ts"]
  },
  "linter": {
    "rules": {
      "correctness": {
        "noChildrenProp": "off"
      },
      "nursery": {
        "noHeadElement": "off"
      },
      "style": {
        "useFilenamingConvention": "warn"
      }
    }
  }
}
