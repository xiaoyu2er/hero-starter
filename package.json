{
  "name": "hero-tanstack-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "generate": "npx @better-auth/cli generate --output ./prisma/schema.prisma --config ./src/lib/auth.ts",
    "migrate": "npx prisma format && npx prisma generate && npx prisma db push",
    "migrate:prod": "dotenv -e .env.prod -- npx prisma db push deploy",
    "email:dev": "email dev --dir src/emails --port 7006"
  },
  "dependencies": {
    "@heroui/react": "^2.7.5",
    "@iconify/react": "^5.2.0",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-email/components": "^0.0.34",
    "@tanstack/react-form": "^1.1.0",
    "@tanstack/react-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.68.0",
    "@tanstack/react-router": "^1.114.17",
    "@tanstack/react-router-devtools": "^1.114.21",
    "@tanstack/react-router-with-query": "^1.114.22",
    "@tanstack/react-start": "^1.114.19",
    "@tanstack/zod-adapter": "^1.114.23",
    "better-auth": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.482.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redaxios": "^0.5.1",
    "resend": "^4.1.2",
    "tailwind-merge": "^2.6.0",
    "usehooks-ts": "^3.1.1",
    "uuid": "^11.1.0",
    "vinxi": "0.5.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "postcss": "^8.5.1",
    "prisma": "^6.5.0",
    "react-email": "^3.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "ultracite": "^4.1.21",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
